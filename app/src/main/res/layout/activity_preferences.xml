<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:background="@color/colorPrimary" android:layout_width="match_parent"
        android:layout_height="match_parent" android:id="@+id/coordinatorLayout2">

    <com.google.android.material.appbar.AppBarLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:theme="@style/AppTheme"
            android:id="@+id/appBarLayout2" app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="360dp"
            app:layout_constraintEnd_toEndOf="parent"/>


    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="52dp" android:id="@+id/frameLayout"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="48dp"
            app:layout_constraintStart_toStartOf="parent">


        <Switch
                android:enabled="true"
                android:layout_width="124dp"
                android:layout_height="36dp" android:id="@+id/switch1"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="8dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="210dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="52dp" android:id="@+id/frameLayout1"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="108dp">


        <TextView
                android:textAlignment="center"
                android:text="Language (язык)"
                android:textSize="18sp"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" android:id="@+id/textView15"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="16dp"/>
        <Spinner
                android:layout_width="185dp"

                android:layout_height="wrap_content"
                android:id="@+id/spinner" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="16dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="52dp" android:id="@+id/frameLayout2a"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="167dp">


        <EditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:inputType="number"
                android:textAppearance="?android:attr/textAppearanceLarge"


                 android:textAlignment="center"
                android:ems="10"
                android:id="@+id/editText3"
                tools:layout_editor_absoluteX="8dp" tools:layout_editor_absoluteY="181dp"/>
        <TextView
                android:textAlignment="center"
                android:text="Year min"
                android:textSize="18sp"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" android:id="@+id/textView16"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="16dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="52dp" android:id="@+id/frameLayout3"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="227dp" app:layout_constraintStart_toStartOf="parent">


        <EditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:inputType="number"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textAlignment="center"

                android:ems="10"
                android:id="@+id/editText4"
                tools:layout_editor_absoluteY="181dp"/>
        <TextView
                android:textAlignment="center"
                android:text="Year max"
                android:textSize="18sp"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" android:id="@+id/textView17"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="16dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="52dp" android:id="@+id/frameLayout4"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="286dp" app:layout_constraintStart_toStartOf="parent">


    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="52dp" android:id="@+id/frameLayout2b"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="468dp"
            app:layout_constraintHorizontal_bias="0.0">


        <Button
                android:text="Save settings"
                android:layout_width="344dp"
                android:layout_height="wrap_content" android:id="@+id/button"
                tools:layout_editor_absoluteY="2dp" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="52dp" android:id="@+id/frameLayout2c"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="408dp"
            app:layout_constraintHorizontal_bias="1.0">


    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="52dp" android:id="@+id/frameLayout2d"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="348dp"
            app:layout_constraintHorizontal_bias="0.0">


    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>